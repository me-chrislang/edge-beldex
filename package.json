    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "flow-bin": "^0.89.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "mocha": "^4.0.0",
    "node-fetch": "^2.1.2",
    "nyc": "^11.3.0",
    "prettier": "^2.2.0",
    "rimraf": "^2.6.2",
    "sucrase": "^3.9.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
