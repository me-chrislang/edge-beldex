  "main": "./index.js",
  "module": "./lib/xmrIndex.js",
  "files": [
    "CHANGELOG.md",
    "index.js",
    "lib/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib && sucrase ./src -q -d ./lib -t flow && webpack",
    "fix": "npm run lint -- --fix",
