    "beldex-core-js":"https://github.com/Beldex-Coin/beldex-core-js.git#360948d9b6222b8d24188ce395c67249aa358050",
    "uri-js": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.1.2",
    "edge-core-js": "^0.17.33",
